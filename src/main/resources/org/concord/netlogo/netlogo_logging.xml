<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE log4j:configuration SYSTEM "log4j.dtd">

<log4j:configuration debug="false"
  xmlns:log4j='http://jakarta.apache.org/log4j/'>
  
  <appender name="Null" class="org.apache.log4j.varia.NullAppender" />

  <category name="org.nlogo.log.Logger.GLOBALS">
    <priority value="info" />
  </category>

  <category name="org.nlogo.log.Logger.GREENS">
    <priority value="info" />
  </category>

  <category name="org.nlogo.log.Logger.CODE">
    <priority value="info" />
  </category>

  <category name="org.nlogo.log.Logger.WIDGETS">
    <priority value="info" />
  </category>

  <category name="org.nlogo.log.Logger.BUTTONS">
    <priority value="info" />
  </category>

  <category name="org.nlogo.log.Logger.SPEED_SLIDER">
    <priority value="info" />
  </category>

  <category name="org.nlogo.log.Logger.TURTLES">
    <priority value="info" />
  </category>

  <category name="org.nlogo.log.Logger.LINKS">
    <priority value="info" />
  </category>
  
  <root>
    <priority value="info" />
    <appender-ref ref="Null" />
  </root>

</log4j:configuration>
